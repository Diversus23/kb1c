        #КонецЕсли
     )
#ИначеЕсли "#Параметр(45)" = "Объект" ИЛИ "#Параметр(45)" = " " #Тогда
      #Параметр(46) &lt;&gt; Неопределено
    И 
  // Предполагается, что право Чтение может зависеть только от права Чтение
  #Если "#Параметр(2)" = "Чтение" #Тогда
    (
        // Проверка права Чтение
        НЕ
        (
              НЕ Истина В
